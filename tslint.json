{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier",
    "tslint-react"
  ],
  "rules": {
    "jsx-wrap-multiline": null,
    "jsx-alignment": false,
    "jsx-curly-spacing": false,
    "jsx-no-multiline-js": false,
    "jsx-equals-spacing": false,
    "prettier": true
  }
}
